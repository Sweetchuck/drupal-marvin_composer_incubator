        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^5.3",
        "weitzman/drupal-test-traits": "dev-master#b902b579a99393dabd7eaad4f0041c9da7e29152"
    },
    "suggest": {
